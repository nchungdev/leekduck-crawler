{
  "github": {
    "user": "nchungdev",
    "repo": "leakduck-crawler"
  },
  "scraper_settings": {
    "retries": 3,
    "delay": 5,
    "timeout": 15,
    "cache_expiration_hours": 1
  },
  "scrapers": {
    "RaidNowScraper": {
      "url": "https://raidnow.leekduck.com",
      "file_name": "raid_now",
      "enabled": false,
      "check_existing": false,
      "schedule": {
        "type": "raid_now",
        "every_minutes": 20,
        "active_hours": [
          8,
          22
        ]
      }
    },
    "RaidBossScraper": {
      "url": "https://leekduck.com/boss/",
      "file_name": "raid_bosses",
      "enabled": true,
      "schedule": {
        "type": "default_12h"
      }
    },

    "ResearchScraper": {
      "url": "https://leekduck.com/research/",
      "file_name": "research_tasks",
      "enabled": true,
      "schedule": {
        "type": "default_12h"
      }
    },
    "RocketLineupScraper": {
      "url": "https://leekduck.com/rocket-lineups/",
      "file_name": "rocket_lineups",
      "enabled": true,
      "schedule": {
        "type": "default_12h"
      }
    },
    "EggScraper": {
      "url": "https://leekduck.com/eggs/",
      "file_name": "egg_pool",
      "enabled": true,
      "schedule": {
        "type": "default_12h"
      }
    },
    "EventScraper": {
      "url": "https://leekduck.com/events/",
      "file_name": "events",
      "enabled": true,
      "check_existing": false,
      "schedule": {
        "type": "default_12h"
      }
    }
  }
}